						["use_absorbMode"] = true,
						["useGroup_count"] = false,
						["spellIds"] = {
							288212, -- [1]
						},
						["custom"] = "function(allstates, e, u)\n    if e == \"INSTANCE_ENCOUNTER_ENGAGE_UNIT\" then\n        for _, state in pairs(allstates) do\n            state.show = false;\n            state.changed = true;\n        end\n        for i = 1,5 do\n            local unit = \"boss\"..i\n            local guid = UnitGUID(unit)\n            if guid and aura_env.guids[select(6, strsplit(\"-\", guid))] then\n                local state = allstates[guid]\n                local current = UnitPower(unit)\n                if not state and current >= 95 and current < 100 then\n                    allstates[guid] = {\n                        show = true,\n                        changed = true,\n                        progressType = \"static\",\n                        energy = current,\n                    }\n                end\n            end\n        end\n    end\n    if e == \"UNIT_POWER_UPDATE\" then\n        local guid = UnitGUID(u)\n        if guid then\n            if aura_env.guids[select(6, strsplit(\"-\", guid))] then\n                local state = allstates[UnitGUID(u)]\n                if state then\n                    local current = UnitPower(u)\n                    state.energy = current\n                    state.changed = true\n                    if current < 95 or current >= 100 then\n                        state.show = false\n                        state.changed = true\n                    end\n                end\n                if not state then\n                    local current = UnitPower(u)\n                    if current >= 95 and current < 100 then\n                        allstates[guid] = {\n                            show = true,\n                            changed = true,\n                            progressType = \"static\",\n                            energy = current,\n                        }\n                    end\n                end\n            end\n        end\n    end\n    return true\nend",
