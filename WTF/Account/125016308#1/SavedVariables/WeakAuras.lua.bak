		["Raden Unstable Vita Rotation Notifer Method"] = {
			["xOffset"] = 1.0000610351563,
			["preferToUpdate"] = false,
			["customText"] = "function()\n    if(aura_env.nextUp)then\n        return \"NEXT\";\n    else\n        return aura_env.holding and \"HOLD\" or \"VITA INC\";\n    end\nend",
			["yOffset"] = 247.00006103516,
			["anchorPoint"] = "CENTER",
			["cooldownSwipe"] = true,
			["cooldownEdge"] = false,
			["icon"] = true,
			["triggers"] = {
				{
					["trigger"] = {
						["type"] = "custom",
						["custom_type"] = "event",
						["custom_hide"] = "custom",
						["event"] = "Health",
						["subeventPrefix"] = "SPELL",
						["customDuration"] = "function()\n    if aura_env.holding then\n        return aura_env.duration,GetTime() + aura_env.duration;\n    else\n        return aura_env.myIndex*aura_env.duration,GetTime() + (aura_env.duration*aura_env.myIndex);\n    end\nend\n\n\n",
						["custom"] = "function(event,...)\n    if(event == \"ENCOUNTER_START\")then\n        \n        local difficultyID = select(3,...);\n        if (difficultyID == 16)then\n            aura_env.duration = 6;\n        else\n            aura_env.duration = 7;\n        end\n        \n    elseif(event == \"WA_RADEN_VITA_UPDATE\")then\n        local vitaRunners,currentHolder = ...\n        \n        aura_env.holder = currentHolder;\n        aura_env.vitaRunners = vitaRunners;\n        \n        --New vita reset.\n        if(#vitaRunners == aura_env.runnerCountReset)then\n            aura_env.myIndex = nil;\n            aura_env.holding = nil;\n            aura_env.nextUp = nil;\n            aura_env.region.subRegions[2]:SetVisible(false);\n        end\n        \n        if(vitaRunners[1] == WeakAuras.me)then\n            aura_env.nextUp = true;\n            -- yikes dont look under this. Forces a text update wihtout trigger update.\n            aura_env.region.subRegionEvents:Notify(\"Update\");\n            aura_env.region.subRegions[2]:SetVisible(true);\n        end\n        \n        if(aura_env.holder == WeakAuras.me)then\n            aura_env.holding = vitaRunners[#vitaRunners] ~= WeakAuras.me;\n            aura_env.nextUp = nil;\n            \n            aura_env.region.subRegions[2]:SetVisible(true);\n            return true;\n            \n        elseif (not aura_env.myIndex) then\n            aura_env.holding = false;\n            for runnerIndex,runner in ipairs(vitaRunners)do\n                if (runner == WeakAuras.me)then\n                    aura_env.myIndex = runnerIndex;\n                    return true;\n                end\n            end\n        else\n            aura_env.holding = false;\n        end\n    elseif(event == \"CHAT_MSG_ADDON\")then\n        local prefix,furthestPlayer,channel = ...;\n        if(prefix == aura_env.addonMsgPrefix and channel == \"RAID\")then\n            if(aura_env.nextUp)then\n                local colorRed,colorGreen = {1,0,0,1},{0,1,0,1};\n                \n                if(guy == WeakAuras.me)then\n                    color = colorGreen;\n                else\n                    color = colorRed;\n                end\n                \n                aura_env.region.subRegions[2]:SetGlowColor(unpack(color));\n            end\n        end\n    end\nend\n\n\n\n\n",
